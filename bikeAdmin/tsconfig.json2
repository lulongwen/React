{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true
  },
  "exclude": [
    "node_modules"
  ],
  "baseUrl":"src",
  "paths": {
    "src/*":["src/*"],
    "components/*":["src/app/components/*"],
    "common/*":["src/common/*"],
    "api/*":["src/api/*"],
    "reuse/*":["src/app/reusablecomponents/*"],
    "actions/*":["src/app/actions/*"],
    "reducers/*":["src/app/reducers/*"],
    "store/*":["src/app/store/*"]
  }
}
